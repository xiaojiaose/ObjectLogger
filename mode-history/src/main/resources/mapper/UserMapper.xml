<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.history.mapper.UserMapper">
    <resultMap id="BaseResultMap" type="com.example.history.entity.User">
        <id column="id" jdbcType="BIGINT" property="id" />
    </resultMap>

    <select id="findAll" resultMap="BaseResultMap">
        SELECT * FROM tb_user
    </select>

    <insert id="addUser" parameterType="com.example.history.entity.User">
        insert into tb_user(id,username,password)
        values(#{user.id,jdbcType=BIGINT},#{user.username,jdbcType=VARCHAR},#{user.password,jdbcType=VARCHAR});

    </insert>

</mapper>